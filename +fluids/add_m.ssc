component add_m
% PS Add (m) :0.75
% This block adds signals of the two inputs:
%
% y = u_1 + u_2
%
% All connections are physical signal ports.

% Copyright 2005-2018 The MathWorks, Inc.

  inputs
    I1 = {0,'m'}; % :left
    I2 = {0,'m'}; % :left
  end

  outputs
    O = {0,'m'}; % :right
  end
  
  equations
    O == I1 + I2;
  end

end
